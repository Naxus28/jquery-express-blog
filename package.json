{
  "name": "16-blog-api-challenge",
  "version": "1.0.0",
  "description": "To close out this lesson, you'll build a CRUD API for a blogging platform.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build-server": "babel server -d dist -s --presets env,stage-0",
    "build": "npm run clean && npm run build-server",
    "start": "npm run build && node dist/index",
    "start:dev": "PORT=8080 nodemon server/index --exec babel-node",
    "test": "PORT=5000 mocha --require babel-register --exit"
  },
  "author": "Gabriel Ferraz <gabrielferraz27@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-env": "^2.4.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-env": "^2.4.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "uuid": "^3.2.1"
  },
  "engines": {
    "node": "^7.3.0"
  }
